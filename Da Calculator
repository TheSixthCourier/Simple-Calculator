operand_one = 0
operand_tw0 = 0
sum = 0.0
difference = 0
quotient = 0
product = 0
remainder = 0.0 #use "%" symbol to get remainder


operand_one = int(input("Input the first Operand: "))
operand_two = int(input("Input the first Operand: "))

sum = operand_one + operand_two
sum = round(sum, 1)

difference = operand_one - operand_two
difference = round(sum, 1)

product = operand_one * operand_two
product = round(sum, 1)

quotient = operand_one / operand_two
quotient = round(sum, 1)
